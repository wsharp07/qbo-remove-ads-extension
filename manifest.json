{
  "manifest_version": 3,
  "name": "QBO Remove Ads",
  "version": "1.1",
  "description": "Removes Marketing Advertisements from QuickBooks Online. If you pay for Quickbooks Online, you shouldn't have to see ads.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "run_at": "document_end",
      "matches": [
        "https://app.qbo.intuit.com/app/*"
      ]
    }
  ],
  "options_ui": {
    "page": "scripts/options.html",
    "open_in_tab": false
  }
}
